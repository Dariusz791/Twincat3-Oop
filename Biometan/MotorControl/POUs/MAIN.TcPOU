<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.12">
  <POU Name="MAIN" Id="{94995db9-82fb-4583-94ae-a1fd1a7e3270}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM MAIN
VAR
	
	// Drive
	fbDrive			          : FB_Drive;
	fbDriveReverse	          : FB_DriveReverse;
	bUnitId              	  : BYTE:=1; //adres napędu falownikowego Higienizatora
	
	fbDriveInverter1		  : FB_DriveInverter(1);
	//fbDriveInverter2		  : FB_DriveInverter(2);

	//Drive Control	
	fbMotorControl 	     	  : FB_MotorControl(fbDrive);
	fbMotorControlReverse 	  : FB_MotorControlReverse(fbDriveReverse);
	fbMotorControlInverter    : FB_MotorControlInverter(fbDriveInverter1);
	
//fbMotorControlInverter2    : FB_MotorControlInverter(fbDriveInverter2);	
	
	

	//Modbus RTU - obiektów tej klasy może być tylko tyle ile jest dostępnych fizycznych portów RS485
	//np. dla modułu EL6022 są dwa fizyczne porty RS485
	fbInverterOperation		   : FB_InverterOperation;
	

	
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[mergeIO();

fbMotorControl();
fbMotorControlReverse();
fbMotorControlInverter();

//fbMotorControlInverter2();


fbInverterOperation();



]]></ST>
    </Implementation>
  </POU>
</TcPlcObject>